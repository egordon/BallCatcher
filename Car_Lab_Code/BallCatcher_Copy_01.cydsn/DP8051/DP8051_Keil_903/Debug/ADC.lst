1004   2              }
1005   1              else
1006   1              {
1007   2                  resolution = ADC_CFG4_RESOLUTION;
1008   2              }
1009   1          
1010   1          if(resolution < 12)
1011   1          {
1012   2              A = 1000000;
1013   2              B = 1;
1014   2          }
1015   1          else if(resolution < 15)
1016   1          {
1017   2              A = 100000;
1018   2              B = 10;
1019   2          }
1020   1          else if(resolution < 18)
1021   1          {
1022   2              A = 10000;
