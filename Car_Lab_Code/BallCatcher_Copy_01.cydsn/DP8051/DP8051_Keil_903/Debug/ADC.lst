1024   2          }
1025   1          else
1026   1          {
1027   2              A = 1000;
1028   2              B = 1000;
1029   2          }
1030   1      
1031   1          /* Subtract ADC offset */
1032   1          adcCounts -= ADC_Offset;
1033   1      
1034   1          uVolts = ((A * adcCounts) / ((int32)ADC_CountsPerVolt/B)) ;   
